{
    "env": {
        "browser": true,
        "es6": true,
        "jest/globals": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "jest"
    ],
    "rules": {
        "react/jsx-filename-extension": 0,
        "import/no-extraneous-dependencies": ["error", {"devDependencies": ["**/factories/**", "**/__tests__/**", "**/__mocks__/**", "webpack.config.js"]}],
        "react/prop-types": 0
    }
}
